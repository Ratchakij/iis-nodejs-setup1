<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <iisnode nodeProcessCommandLine="C:\Program Files\nodejs\node.exe" />
        <handlers>
            <add name="iisnode" path="src/app.js" verb="*" modules="iisnode" />
        </handlers>

        <rewrite>
            <rules>
                <rule name="myapp">
                    <match url="/*" />
                    <action type="Rewrite" url="src/app.js" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>